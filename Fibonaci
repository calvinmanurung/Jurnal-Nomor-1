public class Fibonaci{
	public int fibo(int i){
	if(i==0){
		return 0;
	
		}
		else if(i==1){
		return 1;
	}
		else{
			return fibo(i-1)+fibo(i-2);
		}
	}
	public static void main(String[] args){
		Fibonaci f=new Fibonaci();
		
		int input=4;
		int jumlah=0;
		for(int i=1;i<=input;i++){
		jumlah+=f.fibo(i);
		}
		System.out.print(jumlah);
		}
	}
